{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-return-await": ["off"],
    "import/prefer-default-export": ["off"]
  }
}
